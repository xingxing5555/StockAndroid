<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FilletBtView">
        <attr name="fillet" format="dimension" />
        <attr name="defaultColor" format="color" />
        <attr name="pressedColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TitleBar">
        <attr name="leftText" format="reference" />
        <attr name="leftIcon" format="reference" />
        <attr name="rightText" format="reference" />
        <attr name="rightIcon" format="reference" />
        <attr name="bartitle" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="rightColor" format="color" />
        <attr name="showLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TitleBarV3">
        <attr name="left_text" format="reference" />
        <attr name="left_icon" format="reference" />
        <attr name="right_text" format="reference" />
        <attr name="right_icon" format="reference" />
        <attr name="bar_title" format="reference" />
        <attr name="text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleAngleTitleView">
        <attr name="radiusOfCorner" format="dimension" />
        <attr name="frameWidth" format="dimension" />
        <attr name="backColor" format="color" />
        <attr name="frameColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="CircleImageFrColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FilletRelativeLayout">
        <attr name="layoutFillet" format="dimension" />
        <attr name="defaultBackColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TagCenterLineView">
        <attr name="tagCenterRadius" format="dimension" />
        <attr name="tagCenterCircleColor" format="color" />
    </declare-styleable>
    <declare-styleable name="MultiViewPager">
        <!-- Child view inside a page to match the page size against. -->
        <attr name="matchChildWidth" format="reference" />
        <!-- Maximum size. -->
        <attr name="android:maxWidth" />
        <!-- Maximum size. -->
        <attr name="android:maxHeight" />
    </declare-styleable>
    <declare-styleable name="stockList">
        <attr name="type">
            <enum name="optionalStock" value="0" />
            <enum name="sortStock" value="1" />
            <enum name="industryStock" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StickyNavLayout">
        <attr name="titleHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AvgFlowLayout">
        <attr name="avgCount" format="integer" />
        <attr name="verticalDividerWidth" format="dimension" />
        <attr name="horizontalDividerHeight" format="dimension" />
        <attr name="isShowHorizontalDivider" format="boolean" />
        <attr name="isShowVerticalDivider" format="boolean" />
        <attr name="dividerColor" format="color" />
    </declare-styleable>


    <declare-styleable name="FirstTitleView">
        <attr name="firstTitleIcon" format="reference" />
        <attr name="isShowArrow" format="boolean" />
        <attr name="firstTitle" format="string" />
    </declare-styleable>
</resources>
